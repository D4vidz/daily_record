{
  "compilerOptions": {
    "target": "es2016",
    "lib": [
      "es2016"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "inlineSourceMap": true
  },
  "files": [],
  "include": [
    "./*"
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}